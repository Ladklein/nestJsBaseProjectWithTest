{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/libs/utile-routes/(.*)": "<rootDir>/../libs/libs/utile-routes/src/$1",
    "@app/libs/utile-routes": "<rootDir>/../libs/libs/utile-routes/src"
  }
}